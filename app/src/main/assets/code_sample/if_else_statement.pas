program if_else;
var
    num1, num2: integer;
begin
    writeln('enter first number: ');
    readln(num1);
    writeln('enter second number: ');
    readln(num2);
    if (num1 < num2) then
        writeln('num1 < num2')
    else
        writeln('num1 > num2');
    readln;
end.

